require(foreign)
sdemt417<- data.frame(read.dbf("C://Users//azkam mora//Desktop//ENOE//sdemt417.dbf"))
asalariado <- subset(sdemt417,ENT==14 & MH_COL==2)
ocupado <- subset(sdemt417,ENT==14 & CLASE2==1)
ocupados <-nrow(ocupado)
asalariados <-nrow(asalariado)                  
tasaasalariados417 <- (asalariados/ocupados)*100
tasaasalariados417

