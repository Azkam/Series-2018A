serie<- sample(300:1000,120)
seriets<- ts(serie, frequency=12, start = 2002)
tdesco<- decompose(seriets)
plot(tdesco)
names(tdesco)
##Valor serie de tiempo en enero 2013##
seriets
tdesco$trend
tdesco$seasonal
tdesco$random

